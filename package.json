{
  "name": "busbudcodechallenge",
  "version": "0.0.1",
  "description": "coding challenge in Nodejs",
  "main": "dist/app.js",
  "scripts": {
    "dev:tsc:watch": "tsc --watch",
    "dev:ui:watch": "webpack --config ./webpack.config.dev.js --progress --color --watch",
    "dev:server:watch": "ts-node-dev dist/app.js",
    "typeorm:createmigration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -n",
    "typeorm:generatemigration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n",
    "typeorm:runmigration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/ormconfig.ts migration:run",
    "typeorm:revertmigration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "test": "ts-mocha --paths -p tsconfig.json src/**/*.spec.ts",
    "postinstall": "tsc && webpack --config ./webpack.config.dev.js && npm run typeorm:runmigration",
    "start": "ts-node dist/app.js",
    "cleanup": "npm prune --production"
  },
  "author": "Renan Fragoso",
  "license": "WTFPL",
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.1",
    "@types/redis": "^2.8.32",
    "axios": "^0.23.0",
    "bootstrap": "^5.1.3",
    "class-transformer": "^0.4.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "geolib": "^3.3.3",
    "module-alias": "^2.2.2",
    "pg": "^8.7.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0",
    "recoil": "^0.4.1",
    "redis": "^3.1.2",
    "string-similarity": "^4.0.4",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typeorm": "^0.2.38",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": "14.18.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/react": "^17.0.31",
    "@types/sinon": "^10.0.4",
    "@types/supertest": "^2.0.11",
    "babel-loader": "^8.2.3",
    "chai": "^4.3.4",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.4.0",
    "mini-css-extract-plugin": "^2.4.2",
    "mocha": "^9.1.3",
    "react-dom": "^17.0.2",
    "sass": "^1.43.3",
    "sass-loader": "^12.2.0",
    "sinon": "^11.1.2",
    "style-loader": "^3.3.0",
    "supertest": "^6.1.6",
    "ts-loader": "^9.2.6",
    "ts-mocha": "^8.0.0",
    "ts-node-dev": "^1.1.8",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "_moduleAliases": {
    "models": "dist/src/models",
    "services": "dist/src/services",
    "controllers": "dist/src/controllers",
    "repositories": "dist/src/repositories",
    "lib": "dist/src/lib",
    "config": "dist/src/config"
  }
}
